        <!-- Top Face -->
        <path
            android:pathData="M75,120 L125,70 L135,80 L85,130 Z"
            android:fillColor="#FF000000" />

        <!-- S-Outer Segments -->
        
        <!-- Bottom Loop -->
        <path
            android:pathData="M50,160 C110,160 145,160 145,120"
            android:strokeColor="#FF000000"
            android:strokeWidth="16"
            android:strokeLineCap="round"
            android:strokeLineJoin="round"
            android:fillColor="#00000000" />

        <!-- Top Loop + Arrow -->
        <path
            android:pathData="M55,80 C55,40 100,40 150,40"
            android:strokeColor="#FF000000"
            android:strokeWidth="16"
            android:strokeLineCap="round"
            android:strokeLineJoin="round"
            android:fillColor="#00000000" />
            
        <!-- Arrowhead -->
        <path
            android:pathData="M140,20 L170,40 L140,60"
